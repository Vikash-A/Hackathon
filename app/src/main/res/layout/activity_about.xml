<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.jaya.hackthaonproject.About"
    android:orientation="vertical">
    <com.synnapps.carouselview.CarouselView
        android:id="@+id/carouselView_about"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        app:fillColor="#FFFFFFFF"
        app:pageColor="#00000000"
        app:radius="6dp"
        app:slideInterval="2000"
        app:strokeColor="#FF777777"
        app:strokeWidth="1dp"

        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="trans"
        android:onClick="trans"
        />
    <TextView


        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="25dp"
        android:layout_marginTop="13dp"
        android:layout_margin="5dp"
        android:text="About Publishers"
        android:textColor="@color/colorAccent"
        android:textAlignment="center"
        android:fontFamily="sans-serif"
        android:elevation="0dp"
        android:textStyle="italic|bold"/>


    <TextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:text="Sudhanshu Kr Ambastha"
        android:textColor="#000000"
        android:fontFamily="sans-serif"
        android:elevation="0dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        />
    <TextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:layout_marginTop="5dp"
        android:layout_marginStart="10dp"
        android:text="Shubham Choudhary"
        android:textColor="@color/colorPrimary"
        android:fontFamily="sans-serif"
        android:elevation="0dp"

        />
    <TextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:layout_marginTop="5dp"
        android:layout_marginStart="10dp"
        android:text="Jaya Kumari"
        android:textColor="#000000"
        android:fontFamily="sans-serif"
        android:elevation="0dp" />
    <TextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:layout_marginTop="5dp"
        android:layout_marginStart="10dp"
        android:text="Pragya Bharti"
        android:textColor="@color/colorPrimary"
        android:fontFamily="sans-serif"
        android:elevation="0dp" />
    <TextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:layout_marginTop="5dp"
        android:layout_marginStart="10dp"
        android:text="Pinki Kumari"
        android:textColor="#000000"
        android:fontFamily="sans-serif"
        android:elevation="0dp"
       />
    <TextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:layout_marginTop="5dp"
        android:layout_marginStart="10dp"
        android:text="Shivani"
        android:textColor="@color/colorPrimary"
        android:fontFamily="sans-serif"
        android:elevation="0dp"
/>


</LinearLayout>
